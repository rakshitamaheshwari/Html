    "clean": "gulp clean",
    "start": "gulp",
    "start-docker": "nodemon --exec ts-node src/index.ts --config nodemon-docker.json --legacy-watch",
    "dev:debug": "nodemon --inspect src/index.ts",
    "test:e2e": "mocha --reporter spec --require ./node_modules/ts-node/register tests/**/*.test.ts --timeout=30000",
    "test:unit": "mocha --reporter spec --require ts-node/register src/**/*.spec.ts --exit",
    "test:unit-pipeline": "nyc --reporter=lcov --reporter=cobertura mocha --reporter mocha-junit-reporter --require ./node_modules/ts-node/register src/**/*.spec.ts --exit",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate --config ormconfig.dev.js",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create --config",
    "lint": "./node_modules/.bin/eslint ./src/**/*.ts --fix",
    "test:coverage": "nyc --reporter=text mocha --reporter spec --require ts-node/register src/**/*.spec.ts --exit"
  },
